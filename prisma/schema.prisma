// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // Coloque sua URL de conex√£o aqui
}

generator client {
  provider = "prisma-client-js"
}

model Atividade {
  id        String    @id @default(cuid())
  data      String
  nome      String
  inicio    String
  fim       String
  descricao String?
}